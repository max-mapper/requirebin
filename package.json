{
  "name": "requirebin",
  "version": "0.1.0",
  "description": "create programs in the browser using modules from NPM",
  "scripts": {
    "start": "beefy embed.js:embed-bundle.js index.js:bundle.js 8080",
    "build": "browserify embed.js -o embed-bundle.js && browserify index.js -o bundle.js --debug"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:maxogden/requirebin.git"
  },
  "dependencies": {
    "element-class": "0.0.2",
    "jsonp": "0.0.3",
    "javascript-editor": "0.2.0",
    "uglify-js": "2.2.3",
    "browser-module-sandbox": "~1.3.0",
    "github-oauth": "0.0.4",
    "concat-stream": "1.0.0",
    "browser-request": "0.2.0",
    "github-api": "0.7.0",
    "detective": "~2.1.2"
  },
  "engines": {
    "node": ">=0.8.x"
  },
  "devDependencies": {
    "browserify": "~2.23.1"
  }
}
